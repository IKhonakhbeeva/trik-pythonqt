# profile for non-mevis users to link to PythonQt

include ( python.prf )  

INCLUDEPATH += $$PWD/../src

win32::LIBS += $$DESTDIR/../lib/PythonQt-Qt$${QT_MAJOR_VERSION}$${QT_MINOR_VERSION}-Python$${PYTHON_VERSION}{DEBUG_EXT}.lib
unix::LIBS += -L$$DESTDIR/../lib -lPythonQt-Qt$${QT_MAJOR_VERSION}$${QT_MINOR_VERSION}-Python$${PYTHON_VERSION}$${DEBUG_EXT}
